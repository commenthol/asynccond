{
  "name": "asynccond",
  "version": "1.0.1",
  "description": "async series with conditional exit",
  "main": "index.js",
  "engines": {
    "node": ">=0.8.0"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "mocha": "latest",
    "jshint": "latest",
    "istanbul": "latest",
    "jsdox": "latest"
  },
  "scripts": {
    "test": "mocha test/*.mocha.js",
    "cover": "istanbul cover _mocha --report lcov --report text -- -R dot --check-leaks test/*.mocha.js",
    "lint": "jshint --show-non-errors *.js */*.js",
    "doc": "jsdox -o doc index.js",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "clean": "rm -rf doc coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/asynccond.git"
  },
  "keywords": [
    "async",
    "series",
    "error trap",
    "conditional exit"
  ],
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": "commenthol <commenthol@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commenthol/asynccond/issues"
  },
  "homepage": "https://github.com/commenthol/asynccond"
}